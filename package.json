{
  "name": "toolbelt-plugin-Image-scaler",
  "title": "Static Image Scaler",
  "version": "0.0.5",
  "description": "An example template for building Toolbelt plugins.",
  "main": "index.html",
  "config":{
    "width": 500,
    "height": 700,
    "devmode": true,
    "hidden": false,
    "sourcePath":"G:\\DOCS\\Out Loud ANEW\\internal projects\\image-scaler\\_source",
    "outputPath":"G:\\DOCS\\Out Loud ANEW\\internal projects\\image-scaler\\_out",
    "jobs": [
      {
        "name": "Job1",
        "scale": 100,
        "targetKB": 200,
        "minQuality": 40,
        "suffix":"_WIDTHxHEIGHT"
      },
      {
        "name": "forceit",
        "scale": 100,
        "targetKB": -1,
        "minQuality": 40,
        "suffix":"_WIDTHxHEIGHT"
      },
      {
        "name": "forceit2",
        "scale": 100,
        "targetKB": -1,
        "minQuality": 80,
        "suffix":"_WIDTHxHEIGHT"
      }
    ]
  },
  "dependencies": {
    "co": "^4.6.0",
    "del": "^2.2.2",
